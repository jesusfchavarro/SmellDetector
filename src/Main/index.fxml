                                      </yAxis>
                                    </StackedBarChart>
                                    <BarChart layoutX="3.0" layoutY="230.0" prefHeight="244.0" prefWidth="223.0">
                                       <xAxis>
                                          <CategoryAxis side="BOTTOM" />
