<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<?import javafx.stage.Screen?>
<VBox stylesheets="@main.css" prefHeight="${screen.bounds.height}" prefWidth="${screen.bounds.width}" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Controller">
   <fx:define>
      <Screen fx:factory="getPrimary" fx:id="screen" />
   </fx:define>
   <SplitPane dividerPositions="0.27759197324414714, 0.6605351170568562" prefHeight="160.0" prefWidth="200.0">
      <AnchorPane minHeight="0.0" minWidth="0.0"/>
      <AnchorPane minHeight="0.0" minWidth="0.0"/>
      <AnchorPane layoutX="10.0" layoutY="10.0" minHeight="0.0" minWidth="0.0"/>
   </SplitPane>
   <WebView fx:id="webView" prefHeight="200.0" prefWidth="200.0" />
   <Button mnemonicParsing="false" onAction="#load" text="Load" />
</VBox>
