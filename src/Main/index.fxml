            <AnchorPane id="Content" prefHeight="545.0" prefWidth="380.0">
                     <children>
                        <TextFlow prefHeight="565.0" prefWidth="378.0">
                           <children>
                              <TextArea prefHeight="560.0" prefWidth="376.0" />
                           </children>
                        </TextFlow>
                     </children>
            </AnchorPane>
          </content>
