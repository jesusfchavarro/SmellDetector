                  <Accordion prefHeight="569.0" prefWidth="227.0">
                    <panes>
                      <TitledPane animated="false" text="Graficas">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
